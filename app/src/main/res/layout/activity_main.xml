<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="200dp">

        <!-- Content that you want to scroll -->
        <!-- You can add any kind of layout here, like LinearLayout, RelativeLayout, etc. -->
        <!-- Example: -->
        <LinearLayout
            android:id="@+id/linearLayout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            >

            <!-- Your UI elements here -->
            <!-- ... -->

            <TextView
                android:id="@+id/textViewScrow"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:hint="App List"
                android:textAlignment="center"></TextView>

        </LinearLayout>
    </ScrollView>

    <TextView
        android:id="@+id/textViewProcess"
        android:layout_width="match_parent"
        android:layout_height="20dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="2dp"
        android:layout_marginTop="210dp"
        android:layout_marginEnd="-1dp"
        android:hint="Proccess"
        android:textAlignment="center"></TextView>

    <TextView
    android:id="@+id/textViewEXMagisk"
    android:layout_width="match_parent"
    android:layout_height="20dp"
    android:layout_alignParentStart="true"
    android:layout_alignParentEnd="true"
    android:layout_marginStart="2dp"
    android:layout_marginTop="240dp"
    android:layout_marginEnd="-1dp"
    android:hint="Package Magisk"
    android:textAlignment="center"></TextView>

    <TextView
        android:id="@+id/textViewHaven"
        android:layout_width="match_parent"
        android:layout_height="20dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="2dp"
        android:layout_marginTop="270dp"
        android:layout_marginEnd="-1dp"
        android:hint="Haven Magisk"
        android:textAlignment="center"></TextView>

    <TextView
        android:id="@+id/textViewFrida"
        android:layout_width="match_parent"
        android:layout_height="20dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="2dp"
        android:layout_marginTop="300dp"
        android:layout_marginEnd="-1dp"
        android:hint="Haven Frida"
        android:textAlignment="center"></TextView>

    <TextView
        android:id="@+id/textViewFileMagisk"
        android:layout_width="match_parent"
        android:layout_height="20dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="2dp"
        android:layout_marginTop="330dp"
        android:layout_marginEnd="-1dp"
        android:hint="Haven Frida"
        android:textAlignment="center"></TextView>

    <TextView
        android:id="@+id/textViewBuild"
        android:layout_width="match_parent"
        android:layout_height="20dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="2dp"
        android:layout_marginTop="360dp"
        android:layout_marginEnd="-1dp"
        android:hint="Haven Frida"
        android:textAlignment="center"></TextView>

    <TextView
        android:id="@+id/textViewSProccess"
        android:layout_width="match_parent"
        android:layout_height="20dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="2dp"
        android:layout_marginTop="390dp"
        android:layout_marginEnd="-1dp"
        android:hint="Haven Frida"
        android:textAlignment="center"></TextView>

    <Button
        android:id="@+id/button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="67dp"
        android:hint="Run">
        </Button>

</RelativeLayout>